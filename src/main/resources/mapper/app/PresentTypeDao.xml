<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.app.dao.PresentTypeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.app.entity.PresentTypeEntity" id="presentTypeMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="key" column="key"/>
        <result property="createTime" column="create_time"/>
        <result property="status" column="status"/>
    </resultMap>

    <select id="find" resultType="io.renren.modules.app.entity.PresentTypeEntity">
        select id,name FROM tb_present_type;
    </select>
</mapper>