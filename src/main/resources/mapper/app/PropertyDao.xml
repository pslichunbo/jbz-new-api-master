<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.app.dao.PropertyDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.app.entity.PropertyEntity" id="propertyMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="state" column="state"/>
        <result property="sortIndex" column="sort_index"/>
        <result property="createTime" column="create_time"/>
        <result property="alterTime" column="alter_time"/>
        <result property="deleteTime" column="delete_time"/>
        <result property="userId" column="user_id"/>
    </resultMap>

    <select id="allquery" resultType="io.renren.modules.app.entity.PropertyEntity">
        select * from tb_property
    </select>


</mapper>